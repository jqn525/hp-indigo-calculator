  "interpolationGuidance": {
    "description": "For quantities not in the table, use linear interpolation",
    "example": {
      "quantity": 125,
      "efficiency": 0.75,
      "method": "Interpolate between 100 and 150",
      "q1": 100,
      "q2": 150,  
      "power1": 31.62,
      "power2": 42.05,
      "calculation": "31.62 + (125-100) × (42.05-31.62) ÷ (150-100) = 36.84",
      "production": "36.84 × 1.50 = 55.26"
    }
  },
  
  "commonErrorPrevention": {
    "wrong_multiplication": {
      "error": "100 × 0.75 = 75",
      "correct": "100^0.75 = 31.62",
      "note": "Exponent is NOT multiplication"
    },
    "wrong_order": {
      "error": "100^(0.75 × 1.50) = 100^1.125 = 42.17",
      "correct": "(100^0.75) × 1.50 = 31.62 × 1.50 = 47.43", 
      "note": "Calculate power first, then multiply by rate"
    },
    "precision_loss": {
      "error": "Using rounded intermediate values",
      "correct": "Use full precision or lookup table values",
      "note": "Small rounding errors compound in final calculations"
    }
  },
  
  "usageInstructions": {
    "step1": "Identify the efficiency exponent for the product",
    "step2": "Look up the quantity in the appropriate table",
    "step3": "Use the 'production' value directly (already multiplied by 1.50)",
    "step4": "If quantity not in table, interpolate between closest values",
    "step5": "Verify calculation makes sense compared to nearby quantities"
  }
